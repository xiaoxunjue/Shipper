<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.revenant.driver.com.revenant.shipper.ui.activity.AddShuCheActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@null"
            android:drawableLeft="@drawable/search_bar_icon_normal"
            android:hint="请输入手机号"
            android:paddingLeft="10dp"
            android:id="@+id/search_phone"
            android:inputType="phone"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"></EditText>

    </LinearLayout>

    <Button
        style="@style/login_btn_style_default"
        android:layout_width="match_parent"
        android:id="@+id/add_shuche"
        android:layout_height="wrap_content"
        android:text="确认添加"></Button>
</LinearLayout>
